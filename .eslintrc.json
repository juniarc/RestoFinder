{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["off", "unix"],
        "no-console": "off",
        "indent":"off",
        "object-shorthand":"warn",
        "prefer-destructuring":"warn",
        "class-methods-use-this":"off",
        "import/no-unresolved":"off",
        "no-shadow":"warn",
        "eqeqeq":"off",
        "no-underscore-dangle":"warn"
    }
}
